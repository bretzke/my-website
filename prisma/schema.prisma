generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model UserFeedback {
  id        String   @id
  email     String
  name      String
  imageUrl  String?
  message   String
  active    Boolean  @default(false)
  createdAt DateTime @default(now())
}
