generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserFeedback {
  id        String   @id
  email     String   @db.VarChar(255)
  name      String   @db.VarChar(255)
  imageUrl  String?
  message   String
  active    Boolean  @default(false)
  createdAt DateTime @default(now())
}
