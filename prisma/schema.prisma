generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserFeedback {
  id        String   @id
  name      String   @db.VarChar(255)
  siteUrl   String?
  imageUrl  String?
  message   String
  active    Boolean  @default(false)
  pinned    Boolean  @default(false)
  createdAt DateTime @default(now())
}
